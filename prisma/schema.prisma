datasource db {
  provider = "postgresql"
  url = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Users {
  id String @id @default(cuid())
  gamertag String @unique
  createdAt DateTime @default(now())
}

model Circuits {
  id String @id @default(cuid())
  name String @unique
  description String
  location String
  flag String
  winner String
  createdAt DateTime @default(now())
}

model Times {
  id String @default(cuid())
  time String
  gamertag String
  circuit String
  updatedAt DateTime @updatedAt
  createdAt DateTime @default(now())
  @@id([gamertag, circuit])
}